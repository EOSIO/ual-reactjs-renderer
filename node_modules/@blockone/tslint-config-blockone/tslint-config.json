{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "no-console": [true, "log"],
    "semicolon": [true, "never"],
    "ter-indent": [true, 2],
    "trailing-comma": [true, { "esSpecCompliant": true }],
    "quotemark": [true, "single"],
    "member-ordering": [false]
  },
  "rulesDirectory": []
}
